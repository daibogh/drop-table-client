{
  "name": "razzle-examples-with-typescript",
  "version": "1.0.0",
  "license": "MIT",
  "scripts": {
    "start": "razzle start",
    "build": "razzle build",
    "test": "razzle test --env=jsdom",
    "start:prod": "NODE_ENV=production node build/server.js",
    "lint": "eslint --ext 'js,jsx,ts,tsx' .",
    "analyze": "BUNDLE_ANALYZE=true razzle build",
    "check-typescript": "tsc --project .tsconfig --noEmit",
    "prettier": "prettier --write ./src",
    "cc": "create-component",
    "codegen": "npx @manifoldco/swagger-to-ts https://petstore.swagger.io/v2/swagger.json --output ./src/api/types.ts"
  },
  "pre-commit": [
    "prettier",
    "check-typescript"
  ],
  "dependencies": {
    "@loadable/component": "^5.12.0",
    "@loadable/server": "^5.12.0",
    "@reatom/core": "^1.1.3",
    "@reatom/react": "^1.1.3",
    "axios": "^0.19.2",
    "bootstrap": "^4.5.0",
    "classnames": "^2.2.6",
    "compression": "^1.7.4",
    "express": "^4.17.1",
    "express-locale": "^2.0.0",
    "mini-css-extract-plugin": "^0.9.0",
    "offline-plugin": "^5.0.7",
    "postcss-normalize": "^9.0.0",
    "razzle-heroku": "^3.0.0",
    "react": "^16.8.6",
    "react-dom": "^16.8.6",
    "react-helmet-async": "^1.0.6",
    "react-router-config": "^5.1.1",
    "react-router-dom": "^5.0.1",
    "react-use": "^15.2.5",
    "reactstrap": "^8.4.1",
    "swr": "^0.2.3",
    "use-sse": "^2.0.0"
  },
  "devDependencies": {
    "@loadable/babel-plugin": "^5.12.0",
    "@loadable/webpack-plugin": "^5.12.0",
    "@reatom/babel-plugin": "^1.0.0",
    "@reatom/debug": "^1.2.0",
    "@types/axios": "^0.14.0",
    "@types/bootstrap": "^4.5.0",
    "@types/classnames": "^2.2.10",
    "@types/compression": "^1.7.0",
    "@types/express": "^4.17.0",
    "@types/jest": "^23.3.14",
    "@types/loadable__component": "^5.10.0",
    "@types/loadable__server": "^5.12.2",
    "@types/node": "^12.6.6",
    "@types/react": "^16.8.23",
    "@types/react-dom": "^16.8.4",
    "@types/react-helmet-async": "^1.0.3",
    "@types/react-router-config": "^5.0.1",
    "@types/react-router-dom": "^4.3.4",
    "@types/reactstrap": "^8.4.2",
    "@types/webpack-env": "^1.14.0",
    "@typescript-eslint/eslint-plugin": "^3.3.0",
    "@typescript-eslint/parser": "^3.3.0",
    "babel-plugin-module-resolver": "^4.0.0",
    "blueprint-templates-cli": "^0.1.13",
    "eslint": "^7.2.0",
    "eslint-config-prettier": "^6.11.0",
    "eslint-plugin-import": "^2.21.2",
    "eslint-plugin-jsx-a11y": "^6.2.3",
    "eslint-plugin-prettier": "^3.1.4",
    "eslint-plugin-react": "^7.20.0",
    "fork-ts-checker-webpack-plugin": "^5.0.0",
    "pre-commit": "^1.2.2",
    "prettier": "^2.0.5",
    "razzle": "latest",
    "razzle-plugin-babel-ts": "^1.0.1",
    "razzle-plugin-bundle-analyzer": "^1.2.0",
    "razzle-plugin-scss": "^3.1.3",
    "razzle-plugin-serviceworker": "^1.0.3",
    "razzle-plugin-typescript": "^3.0.0",
    "react-dev-utils": "^10.2.1",
    "ts-jest": "^23.10.5",
    "tslint": "^5.18.0",
    "tslint-react": "^4.0.0",
    "typescript": "^3.5.3"
  },
  "jest": {
    "transform": {
      "\\.(ts|tsx)$": "ts-jest",
      "\\.css$": "<rootDir>/node_modules/razzle/config/jest/cssTransform.js",
      "^(?!.*\\.(js|jsx|css|json)$)": "<rootDir>/node_modules/razzle/config/jest/fileTransform.js"
    },
    "testMatch": [
      "<rootDir>/src/**/__tests__/**/*.(ts|js)?(x)",
      "<rootDir>/src/**/?(*.)(spec|test).(ts|js)?(x)"
    ],
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js",
      "json"
    ],
    "collectCoverageFrom": [
      "src/**/*.{js,jsx,ts,tsx}"
    ]
  }
}